<h1>Listing daily_data</h1>

<table>
  <thead>
    <tr>
      <th>Referral source</th>
      <th>Student action</th>
      <th>Notes</th>
      <th>Employee</th>
      <th>Student</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_data.each do |daily_datum| %>
      <tr>
        <td><%= daily_datum.referral_source %></td>
        <td><%= daily_datum.student_action %></td>
        <td><%= daily_datum.notes %></td>
        <td><%= daily_datum.employee %></td>
        <td><%= daily_datum.student %></td>
        <td><%= link_to 'Show', daily_datum %></td>
        <td><%= link_to 'Edit', edit_daily_datum_path(daily_datum) %></td>
        <td><%= link_to 'Destroy', daily_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

